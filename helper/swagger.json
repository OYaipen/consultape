{
    "swagger": "2.0",
    "info": {
        "description": "Consultas de DNI y RUC - **Perú**",
        "version": "1.0.0",
        "title": "Consulta Ruc y Dni - Perú",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "Alex Pariona",
            "email": "lx7pary@gmail.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        }
    },
    "basePath": "/",
    "tags": [
        {
            "name": "RENIEC",
            "description": "Registro Nacional de Identificación y Estado Civil."
        },
        {
            "name": "SUNAT",
            "description": "Superintendencia Nacional de Aduanas y de Administración Tributaria."
        }
    ],
    "schemes": [
        "https",
        "http"
    ],
    "paths": {
        "/public/api/ruc/{ruc}": {
            "get": {
                "tags": [
                    "SUNAT"
                ],
                "summary": "RUC",
                "description": "Retorna información segun el ruc",
                "operationId": "getByRuc",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "ruc",
                        "in": "path",
                        "description": "Ruc de la empresa",
                        "required": true,
                        "example": "20100070970",
                        "schema": {
                            "type": "string",
                            "minLength": 11,
                            "maxLength": 11
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Operacion exitosa",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/RucResponse"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Ruc invalido o no encontrado"
                    }
                }
            }
        },
        "/public/api/ruc": {
            "post": {
                "tags": [
                    "SUNAT"
                ],
                "summary": "RUC Multiple",
                "description": "Retorna informacion segun los rucs",
                "operationId": "getByRucs",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "ruc",
                        "in": "body",
                        "description": "Lista de Rucs",
                        "required": true,
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "minLength": 11,
                                "maxLength": 11
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Operacion exitosa",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "$ref": "#/definitions/RucResponse"
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "No es una lista de rucs"
                    }
                }
            }
        },
        "/public/api/dni/{dni}": {
            "get": {
                "tags": [
                    "RENIEC"
                ],
                "summary": "DNI",
                "description": "Retorna informacion segun el dni",
                "operationId": "getByDni",
                "parameters": [
                    {
                        "name": "dni",
                        "in": "path",
                        "description": "Dni de la persona",
                        "required": true,
                        "example": "46658592",
                        "schema": {
                            "type": "string",
                            "minLength": 8,
                            "maxLength": 8
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Operacion exitosa",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/DniResponse"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Dni invalido o no encontrado"
                    }
                }
            }
        }
    },
    "securityDefinitions": {
        "comision_auth": {
            "type": "oauth2",
            "authorizationUrl": "https://consultape.swagger.io/oauth/dialog",
            "flow": "implicit",
            "scopes": {
                "write:pets": "modify searchpe in your account",
                "read:pets": "read your searchpe"
            }
        },
        "api_key": {
            "type": "apiKey",
            "name": "api_key",
            "in": "header"
        }
    },
    "definitions": {
        "RucResponse": {
            "type": "object",
            "properties": {
                "ruc": {
                    "type": "string",
                    "example": "20123456789"
                },
                "razonSocial": {
                    "type": "string"
                },
                "nombreComercial": {
                    "type": "string"
                },
                "telefonos": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "tipo": {
                    "type": "string"
                },
                "estado": {
                    "type": "string"
                },
                "condicion": {
                    "type": "string"
                },
                "direccion": {
                    "type": "string"
                },
                "departamento": {
                    "type": "string"
                },
                "provincia": {
                    "type": "string"
                },
                "distrito": {
                    "type": "string"
                },
                "fechaInscripcion": {
                    "type": "string",
                    "format": "date"
                },
                "sistEmsion": {
                    "type": "string"
                },
                "sistContabilidad": {
                    "type": "string"
                },
                "actExterior": {
                    "type": "string"
                },
                "actEconomicas": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "cpPago": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "sistElectronica": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "fechaEmisorFe": {
                    "type": "string",
                    "format": "date"
                },
                "cpeElectronico": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "fechaPle": {
                    "type": "string",
                    "format": "date"
                },
                "padrones": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "DniResponse": {
            "type": "object",
            "properties": {
                "dni": {
                    "type": "string",
                    "example": "00000001"
                },
                "nombres": {
                    "type": "string"
                },
                "apellidoPaterno": {
                    "type": "string"
                },
                "apellidoMaterno": {
                    "type": "string"
                },
                "codVerifica": {
                    "type": "string"
                }
            }
        }
    }
}